{
  "presets": [
    ["@babel/preset-env", { "modules": false }],
    ["@babel/preset-react"]
  ],
  "plugins": [
    ["@babel/plugin-transform-class-properties"],
    ["@babel/plugin-proposal-export-default-from"],
    ["@babel/plugin-transform-modules-commonjs", { "allowTopLevelThis": true }]
  ]
}
 
